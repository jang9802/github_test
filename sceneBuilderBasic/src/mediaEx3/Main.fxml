      <ProgressBar fx:id="progressBar" layoutX="371.0" layoutY="201.0" prefHeight="18.0" prefWidth="143.0" progress="0.0" />
      <ProgressIndicator fx:id="progressIndicator" layoutX="402.0" layoutY="240.0" progress="0.0" />
      <Label fx:id="labelTime" layoutX="443.0" layoutY="225.0" text="0/0 sec" />
   </children>
